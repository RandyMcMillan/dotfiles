#forceOrigin
function forceOrigin(){

while read remoteURL; do echo -e "remoteURL = $remoteURL\n"; done < ~/dotfiles/remoteURL
 

   dest=$PWD".git";
    f="${dest##*/}";
    git ls-remote --get-url ssh://git@mac.ftpaccess.cc/Users/git/${f} #&>-
    if [ "$?" -ne 0 ]; #fail test
        then
        git remote -v
        echo failed to read remote
        makeRemote;forceOrigin;
        else
            echo succeeded to read
            echo adding remote deepspace repository
            git remote add deepspace ssh://git@mac.ftpaccess.cc/Users/git/${f};
            echo setting remote url
            git remote set-url --origin ssh://git@mac.ftpaccess.cc/Users/git/${f};
            git remote -v
        fi
    git ls-remote --get-url https://github.com/randymcmillan/${f} #&>-
    if [ "$?" -ne 0 ]; #fail test
        then
        git remote -v
        echo failed to read remote
        makeRemote;forceOrigin;
        else
            echo succeeded to read
            echo adding remote github repository
            git remote add github https://github.com/randymcmillan/${f};
            echo setting remote url
            git remote set-url --addurl origin ssh://git@mac.ftpaccess.cc/Users/git/${f};
            git remote -v
        fi


}
alias forceorigin=forceOrigin
alias fo=forceorigin

#forceLocal
function forceLocal(){
    dest=$PWD".git";
    f="${dest##*/}";
    git ls-remote --get-url ssh://git@192.168.1.10/Users/git/${f} #&>-
    if [ "$?" -ne 0 ]; #fail test
        then
        git remote -v
        echo failed to read remote
        makeRemote;forceOrigin;
        else
            echo succeeded to read
            echo adding "local" deepspace repository
            git remote add deepspace ssh://git@192.168.1.10/Users/git/${f};
            echo setting remote url
            git remote set-url --origin ssh://git@192.168.1.10/Users/git/${f};
            git remote -v
        fi
    git ls-remote --get-url https://github.com/randymcmillan/${f} #&>-
    if [ "$?" -ne 0 ]; #fail test
        then
        git remote -v
        echo failed to read remote
        makeRemote;forceOrigin;
        else
            echo succeeded to read
            echo adding remote github repository
            git remote add github https://github.com/randymcmillan/${f};
            echo setting remote url
            git remote set-url --addurl local ssh://git@192.168.1.10/Users/git/${f};
            git remote -v
        fi


}
