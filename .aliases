#makeRemote
function makeRemote(){

while read remoteURL; do echo -e "remoteURL = $remoteURL\n"; done < ~/dotfiles/remoteURL
    dest=$PWD".git";
    f="${dest##*/}"; 
    ssh git@mac.ftpaccess.cc "git init --bare ${f}";
}
